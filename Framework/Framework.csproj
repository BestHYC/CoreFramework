<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1.26</Version>
    <Description>1.添加Redis默认DB区间,在Framework.Redis空间下
2.添加邮箱手机等验证方式</Description>
    <Authors>Best_Hong</Authors>
    <PackageProjectUrl>https://github.com/BestHYC/CoreFramework.git</PackageProjectUrl>
    <PackageReleaseNotes>1.全局配置扩展
2.基于Dapper的ORM,以及EF仓储层封装
3.锁的基本使用方式
4.统一权限配置
5.基于表达式树的AutoMapper自己实现方式</PackageReleaseNotes>
    <AssemblyVersion>1.0.1.26</AssemblyVersion>
    <FileVersion>1.0.1.26</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CSRedisCore" Version="3.6.3" />
    <PackageReference Include="Dapper" Version="2.0.35" />
    <PackageReference Include="libphonenumber-csharp" Version="8.12.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.4" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.4" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.7.2" />
    <PackageReference Include="NLog.Web.AspNetCore" Version="4.9.2" />
    <PackageReference Include="RabbitMQ.Client" Version="6.1.0" />
  </ItemGroup>

</Project>
